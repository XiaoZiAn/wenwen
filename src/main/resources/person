create table person
(
person_id       varchar(20) default '0' not null
comment '用户ID'
primary key,
person_name     varchar(20) default '0' not null
comment '用户姓名',
person_age      int default '0'         null
comment '用户年龄',
person_sex      varchar(20)             null
comment '用户性别',
person_birthday varchar(20)             null
comment '用户生日'
);